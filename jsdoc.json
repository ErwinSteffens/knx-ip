{
    "sources": {
        "includePattern": ".+\\.js(doc)?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
        "encoding": "utf8",
        "destination": "./doc",
        "recurse": true,
        "readme": "./README.md",
        "verbose": true,
        "template": "node_modules/ink-docstrap/template"
    },
    "plugins": [
        "plugins/underscore"
    ],
    "tags": {
        "allowUnknownTags": true
    },
    "templates": {
        "outputSourceFiles": true,
        "copyright": "GDNet s.p.r.l.",
        "includeDate": false,
        "linenums": true,
        "theme": "slate",
        "syntaxTheme": "dark"
    }
}
